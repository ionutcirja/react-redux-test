{
  "name": "react-redux-test",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --open",
    "lint": "eslint client/**",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "flow": "flow"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./config/enzyme-setup.js",
    "verbose": true,
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/tests-setup/**",
      "!**/coverage/**",
      "!**/webpack.config.js",
      "!**/postcss.config.js",
      "!**/client/index.js",
      "!**/store/**",
      "!**/reducers/index.js"
    ],
    "testMatch": [
      "**/__tests__/?(*.)(spec|test).js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/config/module-mock.js"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "classnames": "^2.2.5",
    "redux": "^3.7.2",
    "react-redux": "^5.0.6",
    "redux-actions": "^2.2.1",
    "reselect": "^3.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "flow-babel-webpack-plugin": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "redux-mock-store": "^1.4.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "babel-jest": "^22.0.4",
    "jest": "^22.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-sass": "^4.13.1",
    "style-loader": "^0.19.1",
    "css-loader": "^0.28.8",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "file-loader": "^1.1.6",
    "autoprefixer": "^7.2.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "ignore-styles": "^5.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1",
    "pre-commit": "^1.2.2",
    "flow-bin": "^0.63.1",
    "deep-freeze": "^0.0.1"
  }
}
